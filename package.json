{
  "name": "template-express",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --env-file .env src/server.js",
    "start": "node src/server.js",
    "db:init": "node --env-file=.env src/scripts/init-db.js",
    "db:setup": "npm run db:init && npm run typeorm:migrate",
    "typeorm:create": "node src/scripts/create-migration.js",
    "typeorm:migrate": "node --env-file=.env src/scripts/typeorm-migrate.js up",
    "typeorm:revert": "node --env-file=.env src/scripts/typeorm-migrate.js down",
    "swagger:generate": "node src/scripts/generate-swagger.js",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down -v",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://zeeshan-uxbert@github.com/zeeshan-uxbert/template-express.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zeeshan-uxbert/template-express/issues"
  },
  "homepage": "https://github.com/zeeshan-uxbert/template-express#readme",
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.896.0",
    "@prisma/client": "^6.16.2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.9",
    "cls-hooked": "^4.2.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-winston": "^4.2.0",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "i18next": "^25.5.2",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.8.1",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "pino": "^9.12.0",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.16.2",
    "rate-limit": "^0.1.1",
    "redis": "^5.8.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "zod": "^4.1.11"
  },
  "description": "",
  "devDependencies": {
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
